{
  "name": "api-lightbox",
  "version": "1.0.0",
  "description": "Lightbox API users",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "db:generate": "npx drizzle-kit generate --config src/drizzle.config.ts",
    "db:migrate": "npx drizzle-kit migrate --config src/drizzle.config.ts",
    "db:up": "npx drizzle-kit up --config src/drizzle.config.ts",
    "db:push": "npx drizzle-kit push --config src/drizzle.config.ts",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@electric-sql/pglite": "^0.2.13",
    "drizzle-kit": "^0.28.1",
    "drizzle-orm": "^0.36.3",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
